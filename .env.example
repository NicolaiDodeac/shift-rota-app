# Required for NextAuth (set on Vercel)
NEXTAUTH_URL=https://YOUR_DEPLOY_URL
AUTH_SECRET=generate_a_random_secret

# Google OAuth (create OAuth 2.0 Client ID - Web)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Optional: force default timezone
NEXT_PUBLIC_DEFAULT_TZ=Europe/London
AUTH_TRUST_HOST = true
DATABASE_URL="postgresql://USER:PASSWORD@HOST:PORT/DB?sslmode=require"
